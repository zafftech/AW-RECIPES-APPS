<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageButton
                android:id="@+id/imageButton8"
                android:layout_width="268dp"
                android:layout_height="96dp"
                android:contentDescription="@string/kulitayam_desc"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.727"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.009"
                app:srcCompat="@drawable/rectangle" />

            <TextView
                android:id="@+id/editTextText2"
                android:layout_width="214dp"
                android:layout_height="73dp"
                android:ems="10"
                android:inputType="text"
                android:text="Ayam Gunting"
                android:textColor="#FFFFFF"
                android:textSize="30sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.7"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0" />

            <ImageButton
                android:id="@+id/ImageBackButton"
                android:layout_width="76dp"
                android:layout_height="69dp"
                android:contentDescription="@string/kulitayam_desc"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.047"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.407"
                app:srcCompat="@android:drawable/ic_menu_revert" />


        </androidx.constraintlayout.widget.ConstraintLayout>

        <ImageButton
            android:id="@+id/imageButtonAyamGContent"
            android:layout_width="match_parent"
            android:layout_height="310dp"
            android:contentDescription="@string/kulitayam_desc"
            app:srcCompat="@drawable/ayamg" />

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="match_parent"
            android:layout_height="437dp"
            android:contentDescription="@string/kulitayam_desc"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/img_8"
            tools:srcCompat="@drawable/img_8" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="251dp"
            android:contentDescription="@string/kulitayam_desc"
            app:srcCompat="@drawable/img" />

    </LinearLayout>

</androidx.core.widget.NestedScrollView>
